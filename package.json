{
  "name": "webvr-agent",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.12.8",
    "cross-env": "^3.1.4",
    "ghpages": "0.0.10",
    "semistandard": "^9.2.1"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "browser-sync start --serveStatic='examples/,src/' --files='src/**' --port 4000 --cors --no-open --no-notify",
    "predev_https": "shx mkdir -p _build && shx openssl req -new -newkey rsa:4096 -sha512 -days 365 -nodes -x509 -subj '/C=US/ST=Denial/L=Springfield/O=Dis/CN=www.example.com' -keyout _build/key.pem -out _build/cert.pem",
    "dev_https": "npm run dev -- --ssl --cert=_build/cert.pem --key=_build/key.pem",
    "build": "_prod/.",
    "postbuild": "cross-env NODE_ENV=production npm run build",
    "dist": "rollup -c && shx rm -rf _prod && shx cp -R {examples,*.html,*.md,_dist/**} ",
    "lint": "semistandard src",
    "watch": "rollup -c --watch",
    "deploy": "ghpages -p _prod",
    "predeploy": "npm run dist"
  }
}
