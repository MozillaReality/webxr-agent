{
  "name": "webvr-agent",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.16.1",
    "browserify-middleware": "^7.1.0",
    "cross-env": "^3.1.4",
    "express-middleware-rollup": "^1.1.1",
    "feathers": "^2.0.3",
    "feathers-authentication": "^0.7.12",
    "feathers-client": "^1.9.0",
    "feathers-errors": "^2.5.0",
    "feathers-hooks": "^1.7.1",
    "feathers-memory": "^1.1.0",
    "feathers-primus": "^2.0.0",
    "feathers-rest": "^1.6.0",
    "ip": "^1.1.4",
    "primus": "^6.1.0",
    "primus-emitter": "^3.1.1",
    "url-parse": "^1.1.8",
    "window-post-message-proxy": "^0.2.4",
    "ws": "^2.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "semistandard": "^9.2.1",
    "shx": "^0.2.2",
    "snazzy": "^6.0.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "prod": "cross-env NODE_ENV=production node ./server.js",
    "https_key": "shx mkdir -p _build && shx openssl req -new -newkey rsa:4096 -sha512 -days 365 -nodes -x509 -subj '/C=US/ST=Denial/L=Springfield/O=Dis/CN=www.example.com' -keyout _build/key.pem -out _build/cert.pem",
    "lint": "semistandard -v | snazzy",
    "clean": "shx mkdir -p .cache",
    "predev": "npm run clean",
    "preprod": "npm run clean"
  },
  "semistandard": {
    "ignore": [
      "dist/**",
      "examples/**"
    ]
  }
}
